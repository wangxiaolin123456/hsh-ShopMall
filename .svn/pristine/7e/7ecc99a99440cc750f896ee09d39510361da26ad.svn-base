public class BitmapShowBean {
    private Uri uri;
    private int type;
    private String path;
    private String imgid;
    private Uri file;
    private File filelow;

    public void setFilelow(File filelow) {
        this.filelow = filelow;
    }

    public File getFilelow() {
        return filelow;
    }

    public void setFile(Uri file) {
        this.file = file;
    }

    public Uri getFile() {
        return file;
    }

    public String getImgid() {
        return imgid;
    }

    public void setImgid(String imgid) {
        this.imgid = imgid;
    }

    public Uri getUri() {
        return uri;
    }

    public void setUri(Uri uri) {
        this.uri = uri;
    }

    public int getType() {
        return type;
    }

    public void setType(int type) {
        this.type = type;
    }

    public String getPath() {
        return path;
    }

    public void setPath(String path) {
        this.path = path;
    }
}
